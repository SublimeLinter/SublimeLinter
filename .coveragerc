[run]
branch = True
source = SublimeLinter
omit =
    tests/*
    linter-plugin-template/*
    */__init__.py
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
ignore_errors = True

